<!-- START OF   MLP_CTXT -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
    <xs:include schemaLocation="MLP_ID_QOP_RES_SHAPE_v3_4.DTD.xsd"/>
    <xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml.xsd"/>
    <xs:element name="client">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="id"/>
                <xs:element minOccurs="0" ref="pwd"/>
                <xs:element minOccurs="0" ref="serviceid"/>
                <xs:element minOccurs="0" ref="requestmode"/>
                <xs:element minOccurs="0" ref="applicationid"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="sessionid" type="xs:string"/>
    <xs:element name="id" type="xs:string"/>
    <xs:element name="requestor">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="id"/>
                <xs:element minOccurs="0" ref="serviceid"/>
            </xs:sequence>
            <xs:attribute name="type" default="MSISDN">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="MSISDN"/>
                        <xs:enumeration value="IMSI"/>
                        <xs:enumeration value="NAME"/>
                        <xs:enumeration value="E-MAIL"/>
                        <xs:enumeration value="URL"/>
                        <xs:enumeration value="SIPURL"/>
                        <xs:enumeration value="IMS"/>
                        <xs:enumeration value="MDN"/>
                        <xs:enumeration value="ASID"/>
                        <xs:enumeration value="ASPID"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="pwd" type="xs:string"/>
    <xs:element name="serviceid" type="xs:string"/>
    <xs:element name="requestmode">
        <xs:complexType>
            <xs:attribute name="type" default="PASSIVE">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="ACTIVE"/>
                        <xs:enumeration value="PASSIVE"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="applicationid">
        <xs:complexType>
            <xs:attribute name="appProvider" use="required"/>
            <xs:attribute name="appName" use="required"/>
            <xs:attribute name="appVersion"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="subclient">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="id"/>
                <xs:element minOccurs="0" ref="pwd"/>
                <xs:element minOccurs="0" ref="serviceid"/>
            </xs:sequence>
            <xs:attribute name="last_client" default="NO">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="YES"/>
                        <xs:enumeration value="NO"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <!-- END OF SECTION -->
    <!-- START OF   MLP_EME_LIA -->
    <xs:element name="eme_lia">
        <xs:complexType>
            <xs:choice>
                <xs:element maxOccurs="unbounded" ref="eme_pos"/>
                <xs:element ref="req_id"/>
                <xs:sequence>
                    <xs:element ref="result"/>
                    <xs:element minOccurs="0" ref="add_info"/>
                </xs:sequence>
            </xs:choice>
            <xs:attribute name="ver" default="3.4.0">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="3.4.0"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <!-- END OF SECTION -->
    <!-- START OF   MLP_EME_LIR -->
    <xs:element name="eme_lir">
        <xs:complexType>
            <xs:sequence>
                <xs:choice>
                    <xs:element ref="msids"/>
                    <xs:sequence maxOccurs="unbounded">
                        <xs:element maxOccurs="unbounded" ref="msid"/>
                        <xs:element minOccurs="0" ref="gsm_net_param"/>
                        <xs:element minOccurs="0" ref="trans_id"/>
                        <xs:element minOccurs="0" ref="esrd"/>
                        <xs:element minOccurs="0" ref="esrk"/>
                        <xs:element minOccurs="0" ref="supl_support_params"/>
                        <xs:element minOccurs="0" ref="target_serving_node"/>
                    </xs:sequence>
                </xs:choice>
                <xs:element minOccurs="0" ref="eqop"/>
                <xs:element minOccurs="0" ref="geo_info"/>
                <xs:element minOccurs="0" ref="loc_type"/>
                <xs:element minOccurs="0" ref="pushaddr"/>
                <xs:element minOccurs="0" ref="MapRequest"/>
            </xs:sequence>
            <xs:attribute name="ver" default="3.4.0">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="3.4.0"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="res_type" default="SYNC">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="SYNC"/>
                        <xs:enumeration value="ASYNC"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <!-- END OF SECTION -->
    <!-- START OF   MLP_EME_LIREP -->
    <xs:element name="eme_lirep">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="req_id"/>
                <xs:choice>
                    <xs:element maxOccurs="unbounded" ref="eme_pos"/>
                    <xs:sequence>
                        <xs:element ref="result"/>
                        <xs:element minOccurs="0" ref="add_info"/>
                    </xs:sequence>
                </xs:choice>
            </xs:sequence>
            <xs:attribute name="ver" default="3.4.0">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="3.4.0"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <!-- END OF SECTION -->
    <!-- START OF   MLP_EMEREP -->
    <xs:element name="emerep">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="eme_event"/>
                <xs:element minOccurs="0" ref="locationserver_address"/>
            </xs:sequence>
            <xs:attribute name="ver" default="3.4.0">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="3.4.0"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <!-- END OF SECTION -->
    <!-- START OF   MLP_FUNC -->
    <xs:element name="eme_event">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="eme_pos"/>
            </xs:sequence>
            <xs:attribute name="eme_trigger" use="required">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="EME_ORG"/>
                        <xs:enumeration value="EME_REL"/>
                        <xs:enumeration value="EME_HO"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="tlrr_event">
        <xs:complexType>
            <xs:choice>
                <xs:element ref="ms_action"/>
                <xs:element ref="change_area"/>
                <xs:element ref="velocity_event"/>
                <xs:element ref="distance_event"/>
                <xs:element ref="equidistance_event"/>
            </xs:choice>
        </xs:complexType>
    </xs:element>
    <xs:element name="ms_action">
        <xs:complexType>
            <xs:attribute name="type" use="required">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="MS_AVAIL"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="change_area">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="target_area"/>
                <xs:element minOccurs="0" ref="no_of_reports"/>
                <xs:element minOccurs="0" ref="minimumIntervalTime"/>
            </xs:sequence>
            <xs:attribute name="type" use="required">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="MS_ENTERING"/>
                        <xs:enumeration value="MS_LEAVING"/>
                        <xs:enumeration value="MS_WITHIN_AREA"/>
                        <xs:enumeration value="MS_OUTSIDE_AREA"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="loc_estimates" use="required">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="TRUE"/>
                        <xs:enumeration value="FALSE"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="velocity_event">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="target_speed"/>
                <xs:element minOccurs="0" ref="no_of_reports"/>
                <xs:element minOccurs="0" ref="minimumIntervalTime"/>
            </xs:sequence>
            <xs:attribute name="type" use="required">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="INCREASING_ABOVE"/>
                        <xs:enumeration value="ABOVE"/>
                        <xs:enumeration value="DECREASING_BELOW"/>
                        <xs:enumeration value="BELOW"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="vel_estimates" use="required">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="TRUE"/>
                        <xs:enumeration value="FALSE"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="distance_event">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="target_distance"/>
                <xs:element maxOccurs="unbounded" ref="reference_object"/>
                <xs:element minOccurs="0" ref="no_of_reports"/>
                <xs:element minOccurs="0" ref="minimumIntervalTime"/>
            </xs:sequence>
            <xs:attribute name="type" use="required">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="ENTERING_DISTANCE"/>
                        <xs:enumeration value="LEAVING_DISTANCE"/>
                        <xs:enumeration value="WITHIN_DISTANCE"/>
                        <xs:enumeration value="OUTSIDE_DISTANCE"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="loc_estimates" use="required">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="TRUE"/>
                        <xs:enumeration value="FALSE"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="equidistance_event">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="target_equidistance"/>
                <xs:element minOccurs="0" ref="no_of_reports"/>
                <xs:element minOccurs="0" ref="minimumIntervalTime"/>
            </xs:sequence>
            <xs:attribute name="loc_estimates" use="required">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="TRUE"/>
                        <xs:enumeration value="FALSE"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="target_area">
        <xs:complexType>
            <xs:choice>
                <xs:element ref="shape"/>
                <xs:sequence>
                    <xs:element ref="shape"/>
                    <xs:element ref="civicloc"/>
                </xs:sequence>
                <xs:element ref="civicloc"/>
                <xs:element ref="cc"/>
                <xs:element ref="plmn"/>
                <xs:element ref="name_area"/>
            </xs:choice>
        </xs:complexType>
    </xs:element>
    <xs:element name="target_speed">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="speed"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="target_distance" type="xs:string"/>
    <xs:element name="target_equidistance" type="xs:string"/>
    <xs:element name="reference_object">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="msid"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="no_of_reports" type="xs:string"/>
    <xs:element name="minimumIntervalTime" type="xs:string"/>
    <xs:element name="name_area" type="xs:string"/>
    <xs:element name="plmn">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="mcc"/>
                <xs:element ref="mnc"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="interval" type="xs:string"/>
    <xs:element name="loc_type">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="batch_cond"/>
            </xs:sequence>
            <xs:attribute name="type" default="CURRENT">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="CURRENT"/>
                        <xs:enumeration value="LAST"/>
                        <xs:enumeration value="CURRENT_OR_LAST"/>
                        <xs:enumeration value="LAST_OR_CURRENT"/>
                        <xs:enumeration value="INITIAL"/>
                        <xs:enumeration value="CURRENT_AND_INTERMEDIATE"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="per_type" default="REALTIME">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="REALTIME"/>
                        <xs:enumeration value="QUASIREALTIME"/>
                        <xs:enumeration value="BATCH"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="batch_cond">
        <xs:complexType mixed="true">
            <xs:attribute name="batch_type" default="ENDOFSERVICE">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="MEASUREMENTS"/>
                        <xs:enumeration value="MINUTES"/>
                        <xs:enumeration value="ENDOFSERVICE"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="prio">
        <xs:complexType>
            <xs:attribute name="type" default="NORMAL">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="NORMAL"/>
                        <xs:enumeration value="HIGH"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="pushaddr">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="url"/>
                <xs:element minOccurs="0" ref="id"/>
                <xs:element minOccurs="0" ref="pwd"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="req_id" type="xs:string"/>
    <xs:element name="start_time">
        <xs:complexType mixed="true">
            <xs:attribute name="utc_off" default="0000"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="stop_time">
        <xs:complexType mixed="true">
            <xs:attribute name="utc_off" default="0000"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="duration" type="xs:string"/>
    <xs:element name="url" type="xs:string"/>
    <xs:element name="time_remaining" type="xs:string"/>
    <xs:element name="lcs_ref" type="xs:string"/>
    <xs:element name="supl_support_params">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="ip_address"/>
                <xs:element minOccurs="0" ref="shape"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="ip_address" type="xs:string"/>
    <xs:element name="trigger_data">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="req_id"/>
                <xs:element maxOccurs="unbounded" ref="msid"/>
                <xs:element minOccurs="0" ref="tlrr"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="target_serving_node">
        <xs:complexType>
            <xs:choice>
                <xs:element ref="vmscid"/>
                <xs:sequence>
                    <xs:element ref="mme_name"/>
                    <xs:element minOccurs="0" ref="diameter_realm"/>
                </xs:sequence>
                <xs:element ref="sgsnid"/>
                <xs:sequence>
                    <xs:element ref="sgsn_name"/>
                    <xs:element minOccurs="0" ref="diameter_realm"/>
                </xs:sequence>
            </xs:choice>
        </xs:complexType>
    </xs:element>
    <xs:element name="mme_name" type="xs:string"/>
    <xs:element name="sgsnid">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="cc"/>
                <xs:element minOccurs="0" ref="ndc"/>
                <xs:element ref="sgsnno"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="sgsnno" type="xs:string"/>
    <xs:element name="sgsn_name" type="xs:string"/>
    <xs:element name="diameter_realm" type="xs:string"/>
    <xs:element name="query_id" type="xs:string"/>
    <xs:element name="locationserver_address">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="url"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- END OF SECTION -->
    <!-- START OF   MLP_GSM_NET -->
    <xs:element name="gsm_net_param">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="cgi"/>
                <xs:element minOccurs="0" ref="neid"/>
                <xs:element minOccurs="0" ref="nmr"/>
                <xs:element minOccurs="0" ref="ta"/>
                <xs:element minOccurs="0" ref="lmsi"/>
                <xs:element minOccurs="0" ref="imsi"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="cgi">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="mcc"/>
                <xs:element ref="mnc"/>
                <xs:element ref="lac"/>
                <xs:element ref="cellid"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="neid">
        <xs:complexType>
            <xs:choice>
                <xs:element ref="vlrid"/>
                <xs:sequence>
                    <xs:element ref="vmscid"/>
                    <xs:element minOccurs="0" ref="vlrid"/>
                </xs:sequence>
            </xs:choice>
        </xs:complexType>
    </xs:element>
    <xs:element name="vmscid">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="cc"/>
                <xs:element minOccurs="0" ref="ndc"/>
                <xs:element ref="vmscno"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="vlrid">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="cc"/>
                <xs:element minOccurs="0" ref="ndc"/>
                <xs:element ref="vlrno"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="nmr" type="xs:string"/>
    <xs:element name="mcc" type="xs:string"/>
    <xs:element name="mnc" type="xs:string"/>
    <xs:element name="ndc" type="xs:string"/>
    <xs:element name="cc" type="xs:string"/>
    <xs:element name="vmscno" type="xs:string"/>
    <xs:element name="vlrno" type="xs:string"/>
    <xs:element name="lac" type="xs:string"/>
    <xs:element name="cellid" type="xs:string"/>
    <xs:element name="ta" type="xs:string"/>
    <xs:element name="lmsi" type="xs:string"/>
    <xs:element name="imsi" type="xs:string"/>
    <!-- END OF SECTION -->
    <!-- START OF   MLP_HDR -->
    <xs:element name="hdr">
        <xs:complexType>
            <xs:sequence>
                <xs:choice>
                    <xs:element ref="sessionid"/>
                    <xs:sequence>
                        <xs:element ref="client"/>
                        <xs:element minOccurs="0" ref="sessionid"/>
                    </xs:sequence>
                </xs:choice>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="subclient"/>
                <xs:element minOccurs="0" ref="requestor"/>
            </xs:sequence>
            <xs:attribute name="ver" default="3.4.0">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="3.4.0"/>
                    </xs:restriction>
